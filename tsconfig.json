{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom", "es2016"],
    "strict": true,
    "moduleResolution": "bundler",
    "noImplicitAny": false,
    "allowJs": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@figma"
    ],
    "paths": {
      "@/*": [
       "./src/ui/*"
      ]
     }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.vue"
  ]
}